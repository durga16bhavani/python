lower= int(input (" Enter the Lowest Range Value: "))  
upper = int(input (" Enter the Upper Range Value: "))  
  
print ("The Prime Numbers in the range are: ")  
for number in range (lower,upper + 1): 
c=0 
    if number > 1:        
        for i in range (2, number):  
            if (number % i) == 0:  
                break  
        else: 
             c=c+1
            print (c) 
            
