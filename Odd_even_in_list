list=[]
n=int(input("Enter no.of elements:"))
for i in range(n):
   x=int(input())
   list.append(x)
print("List:",list)
even_list=[]
odd_list=[]
for i in range(len(list)):
  if list[i]%2==0:
   even_list.append(list[i])
  else:
    odd_list.append(list[i])
print("odd Numbers List:", odd_list)
print("even Numbers List:",even_list)
