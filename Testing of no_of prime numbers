def isprime(n):
  for i in range(2,n):
    if n%i==0:
      return False
    return True
t=int(input('enter t:'))
for i in range(t):
  n=int(input('enter the number:'))
  count=0
  for i in range(2,n+1):
    if isprime(i):
      count=count+1
      print(i,end=' ')
  print ('\n no.of primes=',count)
